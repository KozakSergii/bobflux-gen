{
    "compilerOptions": {
        "target": "es6",
        "module": "commonjs",
        "declaration": false,
        "noImplicitAny": false,
        "removeComments": false,
        "moduleResolution": "node",
        "noLib": false
    },
    "compileOnSave": false,
    "filesGlob": [
        "./examples/**/*.ts",
        "./cli.ts",
        "./index.ts",
        "./src/*.ts",
        "./spec/*.ts",
        "!./node_modules/**/*",
        "./node_modules/typescript/lib/typescript.d.ts"
    ],
    "files": [
        "./examples/cursors.ts",
        "./examples/state.ts",
        "./cli.ts",
        "./index.ts",
        "./src/cliMain.ts",
        "./src/buildersGenrator.ts",
        "./src/cursorsGenerator.ts",
        "./src/generator.ts",
        "./src/logger.ts",
        "./src/nameUnifier.ts",
        "./src/tsAnalyzer.ts",
        "./src/tsCompilerHost.ts",
        "./src/pathUtils.ts",
        "./src/visitors/bfgVisitor.ts",
        "./src/visitors/nodeVisitor.ts",
        "./src/visitors/classDeclarationVisitor.ts",
        "./src/visitors/enumDeclarationVisitor.ts",
        "./src/visitors/interfaceDeclarationVisitor.ts",
        "./src/visitors/propertyDeclarationVisitor.ts",
        "./src/visitors/propertySignatureVisitor.ts",
        "./src/odata/odata.ts",
        "./src/odata/templates.ts",
        "./src/odata/odataSchema.ts",
        "./src/odata/odataTypeConvertor.ts",
        "./spec/buildersGenrator.spec.ts",
        "./spec/cursorsGenerator.spec.ts",
        "./spec/cursorsGeneratorClasses.spec.ts",
        "./spec/nameUnifier.spec.ts",
        "./spec/pathUtils.spec.ts",
        "./spec/tsAnalyzer.spec.ts",
        "./node_modules/typescript/lib/typescript.d.ts",
        "./typings/tsd.d.ts"
    ]
}
